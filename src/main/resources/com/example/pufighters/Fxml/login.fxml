<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane fx:id="loginAchorpane" xmlns="http://javafx.com/javafx/17.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pufighters.Controllers.LoginController">
      <VBox alignment="CENTER" prefHeight="193.0" prefWidth="189.0" spacing="20.0">
        <padding>
          <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
         <TextField promptText="username" />
         <TextField promptText="password" />
         <Button fx:id="login_button" mnemonicParsing="false" onAction="#onSwitchToHomepage" text="login" />
        <Button onAction="#onHelloButtonClick" text="sign up" />
      
        <Label fx:id="welcomeText" />
      </VBox>
</AnchorPane>
